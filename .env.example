# ================================================
# Agent Dashboard Environment Configuration
# Based on hemonc-agent config.toml
# ================================================

# ============ OpenAI Configuration ============
# Used for: openai_gpt5_1, openai_gpt4o models
OPENAI_API_KEY=sk-your-openai-key
OPENAI_API_BASE=https://api.openai.com/v1

# ============ Local LLM Configuration ============
# Used for: local_llama, local_gpt, local_glm models
LOCAL_API_KEY=EMPTY
LOCAL_API_BASE=http://localhost:11434/v1

# ============ Embedding Model ============
# Model: Qwen3-Embedding-8B
EMBED_API_KEY=EMPTY
EMBED_API_BASE=http://localhost:8080/v1

# ============ Reranker Model ============
# Model: Qwen3-Reranker-8B
RERANKER_API_BASE=http://localhost:8080/rerank

# ============ External Tool API Keys ============
# OncoKB API Token (for gene search)
ONCOKB_TOKEN=your-oncokb-token

# Tavily API Key (for web search)
TAVILY_API_KEY=your-tavily-key

# PubMed API (E-utilities)
PUBMED_API_KEY=your-pubmed-key
PUBMED_EMAIL=your-email@example.com
PUBMED_TOOL=agent-dashboard

# ============ Server Configuration ============
PORT=3006

# ============ ChromaDB Data Paths ============
# Relative to server/tools/ directory
# These should match config.toml paths
CHROMA_ESMO_DB=ESMO_chroma_db_qwen
CHROMA_NCCN_DB=NCCN_chroma_db_qwen
CHROMA_HEMA_DB=HEMA_chroma_db_qwen
CHROMA_PATHO_DB=patho_chroma_db_qwen
CHROMA_WHO_DB=who_chroma_db_qwen
CHROMA_UICC_DB=uicc_chroma_db_qwen0717
